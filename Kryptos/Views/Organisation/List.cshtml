                            function TostringForDate(value) {
                                var test = "" + value;
                                if (test.length == 1) return "0".concat(test);
                                return test;
                            }


                            function getHours12(value) {
                                return TostringForDate((value + 11) % 12 + 1); // edited.
                            }



                            function getMeridiem(value) {
                                return value > 12 ? 'PM' : 'AM';
                            }

