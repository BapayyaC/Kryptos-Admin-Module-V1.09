                                                data:
                                                {
                                                    org: function() {
                                                        return $("#txtOrganizationName").val() + '||||' + recordid;
                                                    }
                                                }
                                            }
                                        },

                                        Email: {
                                            required: true,
                                            email: true,
                                            remote:
                                            {
                                                url: '@Url.Action("CheckIfValidEmail", "Organisation")',
                                                type: "post",
                                                data:
                                                {
                                                    email2: function() {
                                                        return $("#txtEmail").val() + '||||' + recordid;
                                                    }
                                                }
                                            }
                                        },
                                        Phone: {
                                            required: true,
                                            pattern: /^[0-9]{8,15}$/,
                                            remote: {
