                            var recordid;

                            function RecoredEdit(id, pg) {

                                if (id != 0 && pg == 1) {
                                    $("#sp1").html("View");
                                } else if (id != 0 && pg == 2) {
                                    $("#sp1").html("Edit");
                                }

                                recordid = id;
                                ShowHideEdit(false);
                                ShowHideList(true);

                                updateTreegrid();

                                $(function() {
                                    $.ajax({
                                        url: '@Url.Action("getMatchingOrganization", "Organisation")',
                                        type: "Get",
                                        contentType: "application/json; charset=utf-8",
                                        data: { selectedOrg: '' + id + '' },
                                        datatype: "json",
                                        cache: false,
                                        success: function(org) {
                                            UpdateFields(org);
                                            $("Form :input").prop("disabled", false);
                                            if (pg == "1") {
                                                $("Form :input").prop("disabled", true);
                                                $("#btnCancel").prop("disabled", false);
                                                $("#updateSelections").attr("disabled", true);
                                                $("#SelectAll").attr("disabled", true);
                                                $("#deSelectAll").attr("disabled", true);
                                                $("#default-tree").addClass("divdefaulttree");
                                            }
                                            else if(pg=="2")
                                            {
                                                $("#default-tree").removeClass("divdefaulttree");
                                                $("#btnCancel").prop("disabled", false);
                                                $("#updateSelections").attr("disabled", false);
                                                $("#SelectAll").attr("disabled", false);
                                                $("#deSelectAll").attr("disabled", false);
                                            }
                                        },
                                        error: function(xhr, ajaxOptions, thrownError) {
                                            alert(xhr.responseText);
                                        }
                                    });
                                });
                            }


